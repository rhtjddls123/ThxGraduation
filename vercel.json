{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://thinkboo.duckdns.org/api/:path*"
    },
    {
      "source": "/oauth/:path*",
      "destination": "https://thinkboo.duckdns.org/oauth/:path*"
    },
    {
      "source": "/auth/:path*",
      "destination": "https://thinkboo.duckdns.org/auth/:path*"
    },
    {
      "source": "/:path((?!.*\\.).*)",
      "destination": "/"
    }
  ]
}
